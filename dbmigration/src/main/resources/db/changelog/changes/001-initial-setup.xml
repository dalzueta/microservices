<changeSet id="1-create-user-table" author="user">
  <createTable tableName="user">

    <column name="id" type="bigint">
      <constraints primaryKey="true" nullable="false" />
    </column>

    <column name="created_at" type="datetime(6)">
      <constraints nullable="true" />
      <defaultValueComputed>CURRENT_TIMESTAMP(6)</defaultValueComputed>
    </column>

    <!-- Columna password de tipo varchar(255) -->
    <column name="password" type="varchar(255)">
      <constraints nullable="true" />
    </column>

    <!-- Columna token de tipo varchar(255) -->
    <column name="token" type="varchar(255)">
      <constraints nullable="true" />
    </column>

    <!-- Columna token_type de tipo varchar(255) -->
    <column name="token_type" type="varchar(255)">
      <constraints nullable="true" />
    </column>

    <!-- Columna username de tipo varchar(255) -->
    <column name="username" type="varchar(255)">
      <constraints nullable="true" />
    </column>

  </createTable>


</changeSet>